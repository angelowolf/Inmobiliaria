    <include file="struts-servicio.xml"></include>
    <include file="struts-propiedad.xml"></include>
    <include file="struts-ambiente.xml"></include>
    <include file="struts-default.xml"></include>
    <include file="struts-admin.xml"></include>
    <include file="struts-error.xml"></include>
    <include file="struts-tipomoneda.xml"></include>
    <include file="struts-tipopropiedad.xml"></include>
    <include file="struts-contacto.xml"></include>
